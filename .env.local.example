# MongoDB Atlas connection string
# Get this from your MongoDB Atlas cluster
MONGODB_URI=mongodb+srv://username:password@clustername.mongodb.net/?retryWrites=true&w=majority

# MongoDB database name for the application
MONGODB_DB=rag_demo

# OpenAI API key
# Get this from your OpenAI account
OPENAI_API_KEY=your_openai_api_key_here

# Optional API rate limits
# Defaults to 10 requests per minute if not set
RATE_LIMIT_MAX=10
RATE_LIMIT_WINDOW_MS=60000